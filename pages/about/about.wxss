
.team-info{
    display: flex
}
.member-item{
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column;
    width: 50%;
    padding: 34rpx 24rpx;
    font-size: 34rpx;
    color: #333;
    /* border-bottom: 1px solid #e6e6e6; */
    border-right: 1px solid #e6e6e6;

}
.member-item:nth-child(2n){
    border-right: none
}
.member-avatar{
    height: 120rpx;
    width: 120rpx;
    margin-bottom: 16rpx
}
.member-name{
    margin-bottom: 16rpx;
    font-size: 36rpx;
}
.member-sign{
    
    color: #999;
    text-align: center
}
.link-item{
    display: flex;
    justify-content: space-between;
    padding: 30rpx;
    border-bottom: 1px solid #e6e6e6;
}
.link-item:first-child{
    border-top: 1px solid #e6e6e6;
}
.link-item text:last-child{
    color: #999;
}

.part-title{
    background-color:#ebebeb ;
    padding: 24rpx;
    color: #999;
    font-size: 36rpx;
}