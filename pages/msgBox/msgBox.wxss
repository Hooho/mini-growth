
/* 留言板容器  */
.msgBox{
    overflow: hidden;
    background-color: #EBEBEB
}
.msgBox.ShhowBg{
    height: 100vh;
    overflow: hidden
}

/* 留言板 信息  */
.msg-item{
    border-radius: 10rpx;
    box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.4);
    margin: 0 30rpx;
    margin-top: 50rpx;
    min-height: 250rpx;
    color: #fff;
    background-color:#30B099;
}
.msg-item.color-0{
    background-color: #7E7D5B
}
.msg-item.color-1{
    background-color: #7BB3CA
}
.msg-item.color-2{
    background-color: #CDAE51
}
.msg-item.color-3{
    background-color: #C89EC2
}
.msg-item.color-4{
    background-color: #CD9074
}
.msg-item.color-5{
    background-color: #788A9C
}
.msg-item.color-6{
    background-color: #60AE98
}
.msg-item.color-7{
    background-color: #323232
}  
.msg-item.color-8{
    background-color: #DA8A89
}
.msg-item.color-9{
    background-color: #9FB15C
}
.msg-item-title{
    display: flex;
    align-items: center;
    justify-content: flex-start;
    /* flex-flow: column; */
    padding: 20rpx 20rpx;
    padding-left: 0;
    padding-top: 10rpx;
    font-size: 32rpx;
}

.msg-item-title image{
    display:block;
    height:70rpx;
    width:70rpx;
    margin-top:-55rpx;
    border-radius:100%;
    overflow:hidden;
    margin-right:18rpx;

}

.msg-item-content{
    padding: 10rpx 20rpx;
    text-align: center;
}

/* 添加留言板  */
.add-msg-btn{
    position: fixed;
    bottom:20rpx;
    right:20rpx;
    z-index: 3;
    height:80rpx;
    width:80rpx;
    background-color: #fff; 
    overflow: hidden;
    box-shadow: 0 0 20rpx #BFBFBF;
    border-radius: 100%;
    background-image: url(data:image/png;base64,);
    background-repeat:no-repeat;
    background-size: contain;
    transition: 0.5s;
}

/* 关闭留言弹出框  */
.add-msg-btn.close{
    background-image: url(data:image/png;base64,);
    transform: rotateZ(135deg);
}

/* 留言弹出框  */

.comment-board-bg{
    position: fixed;
    top:0;
    right:0;
    left:0;
    bottom:0;
    opacity: 0;
    background-color:rgba(0,0,0,0.4);
    transform: translate3d(0,105%,0);
    transition: opacity 0.3s;
    z-index: 1;
}
.comment-board-dialog{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    padding:40rpx;
    background-color: #fff;
    transform: translate3d(0,105%,0);
    transition: 0.3s ease-in-out;
    box-sizing: border-box;
    z-index: 2;
}
.comment-board.visible .comment-board-bg{
    opacity: 1;
    transform: translate3d(0,0,0);
}

.comment-board.visible  .comment-board-dialog{
    transform: translate3d(0,0,0);
}

.comment-board-textarea{
    height: 120rpx;
    border: 1rpx solid #999;
    border-radius: 10rpx;
    overflow: hidden;
    padding: 20rpx 30rpx;

}
.comment-board-textarea textarea{
    display: block;
    height:100%;
    width:100%;
}
.comment-publish{
    width: 40%;
    color: #fff;
    margin-top: 30rpx;
    font-size: 32rpx;
    background-color: #32B9A2;
}
.comment-publish.active{
    opacity: 0.5
}


/* 加载完毕  */
.allLoaded{
    display: flex;
    align-items: center;
    justify-content:center;
    text-align: center;
    font-size: 32rpx;
    padding: 36rpx 0;
    /* background-color: #E6E6E6; */
    color: #9FA3A6;
}

.allLoaded p{
    height: 1rpx;
    width: 140rpx;
    background-color: #9FA3A6;
    margin: 0 16rpx;
}

/* 加载中  */
.page-infinite-loading{
    text-align: center;
    padding: 36rpx 0;
    color: #9FA3A6;
    font-size: 32rpx;
}